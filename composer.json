{
  "name": "cake2/hello-cake",
  "require": {
    "cakephp/cakephp": "2.10.*",
    "cakedc/utils": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.37",
    "cakephp/debug_kit": ">=2.2.4,<3.0.0"
  },
  "config": {
    "vendor-dir": "vendors/"
  },
  "extra": {
    "installer-paths": {
      "./plugins/{$name}/": [
        "cakephp/debug_kit"
        ]
    }
  }
}
